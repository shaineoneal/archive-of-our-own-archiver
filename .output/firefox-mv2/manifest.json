{
  "manifest_version": 2,
  "name": "Rewritten AO3 Extension",
  "description": "rewrite of AO3 Extension",
  "version": "1.0.0",
  "oauth2": {
    "client_id": "72283065139-0vv4km66vsale3ibeu2dr253gki1r8ln.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": "",
  "permissions": [
    "webRequest",
    "identity",
    "storage",
    "scripting",
    "identity.email",
    "activeTab",
    "http://localhost/*",
    "tabs",
    "*://*.archiveofourown.org/*",
    "https://docs.google.com/spreadsheets/*",
    "*://*.google.com/"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icons/icon-16.png",
    "default_popup": "popup.html",
    "default_title": "AO3 Extension Rewrite's Popup"
  },
  "web_accessible_resources": [
    "js/content_script.css"
  ],
  "commands": {
    "wxt:reload-extension": {
      "description": "Reload the extension during development",
      "suggested_key": {
        "default": "Alt+R"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.archiveofourown.org/*"
      ],
      "run_at": "document_end",
      "css": [
        "content-scripts/content.css"
      ],
      "js": [
        "content-scripts/content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' http://localhost:3000; object-src 'self';"
}