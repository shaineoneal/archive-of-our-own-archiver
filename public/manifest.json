{
  "manifest_version": 3,

  "name": "Rewritten AO3 Extension",
  "description": "rewrite of AO3 Extension",
  "version": "0.1",


  "oauth2": {
    "client_id": "972283065139-htoh4g22degpr245e6eupmbn2ij8ofde.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/spreadsheets", "https://www.googleapis.com/auth/drive", "https://www.googleapis.com/auth/script.external_request"]
  },
  "key": "",
  "permissions": ["webRequest", "identity", "storage", "scripting", "identity.email", "activeTab", "cookies", "notifications"],
  "host_permissions": ["*://*.archiveofourown.org/*", "https://docs.google.com/spreadsheets/*", "*://*.google.com/", "*://*.googleapis.com/*"],

  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },

  "background": {
    "service_worker": "js/background.js"
  },
  
  "action": {
    "default_icon": "icons/icon-16.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
      {
          "matches": ["*://*.archiveofourown.org/*"],
          "run_at": "document_end",
          "js": ["js/content_script.js"],
          "css": ["js/content_script.css"]
      }
  ],
  "web_accessible_resources": [{
    "resources": ["js/content_script.css"],
    "matches": ["*://*.archiveofourown.org/*"]
  },
  {
    "resources": ["js/content_script.js"],
    "matches": ["*://*.archiveofourown.org/*"]
  }]
}
