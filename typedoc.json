{
    "cleanOutputDir": true,
    "commentStyle": "block",
    "darkHighlightTheme": "dark-plus",
    "entryPoints": ["src/index.ts"],
    "exclude": [ "**/dist/**", "node_modules" ],
    "excludePrivate": true,
    "excludeProtected": true,
    "name": "Archive of Our Own Archiver",
    "navigation": {
        "includeFolders": true,
        "includeCategories": true
    },
    "out": "./archive-of-our-own-archiver.wiki",
    "plugin": [ "@droppedcode/typedoc-plugin-copy-assets" ],
    "sort": ["source-order"],
    "tsconfig": "tsconfig.json",
    "copyAssets": {
        "onlyImages": false,
        "exclude": [],
        "includePath": [".(png|md)$"],
        "excludePath": [".*?exclude/.*?$"],
        "copyHtmlImgTag": false
    }
}